SELECT md5(md5(CASE WHEN n.nspname ::text IS NULL THEN ' ' ELSE n.nspname::text END) ||md5(CASE WHEN p.oid ::text IS NULL THEN ' ' ELSE p.oid::text END) ||md5(CASE WHEN t.typname  ::text IS NULL THEN ' ' ELSE t.typname::text END)||md5(CASE WHEN t.typdelim::text IS NULL THEN ' ' ELSE t.typdelim ::text END  ) ::text) AS guid  ,    n.nspname AS schemaname     ,proname AS objectname,         g.usename as ownername,'f' as objecttype  ,     d.description       FROM pg_proc p     JOIN pg_type t on p.prorettype = t.oid     LEFT JOIN pg_description d on p.oid = d.objoid     LEFT JOIN pg_namespace n on n.oid = p.pronamespace     LEFT JOIN pg_user as g ON (g.usesysid =p.proowner)     WHERE n.nspname LIKE 'emcas%'